{
  "name": "backend-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test-cov": "LOG=no jest --coverage --runInBand --forceExit --detectOpenHandles",
    "watch": "nodemon . | pino-pretty",
    "start": "node . | pino-elasticsearch -i backend_node --host http://elastic.asacreative.com",
    "build": "tsc --build tsconfig.json",
    "build:watch": "tsc --build -w tsconfig.json",
    "test": "tsc --build tsconfig.json; jest --coverage --runInBand --forceExit --detectOpenHandles",
    "lint": "tslint -c tslint.json ./**/**/*.ts",
    "precommit": "npm run build && npm run lint && npm run test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@knax/objection-soft-delete": "^1.0.8",
    "@koa/cors": "^3.0.0",
    "dotenv": "^8.2.0",
    "fastest-validator": "^1.0.2",
    "fs-extra": "^8.1.0",
    "knex": "^0.20.10",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-jwt": "^3.6.0",
    "koa-router": "^8.0.8",
    "luxon": "^1.22.0",
    "mysql": "^2.18.1",
    "objection": "^2.1.3",
    "on-finished": "^2.3.0",
    "pg": "^7.18.2",
    "pino": "^5.16.0",
    "pino-elasticsearch": "^4.4.0",
    "pino-pretty": "^3.6.0",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/koa-router": "^7.4.0",
    "@types/luxon": "^1.21.0",
    "@types/on-finished": "^2.3.1",
    "@types/pino": "^5.15.5",
    "@types/uuid": "^7.0.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "typescript": "^3.8.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/services/**/*",
      "!src/modules/index.js"
    ],
    "testEnvironment": "node"
  }
}
