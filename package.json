{
  "name": "backend-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test-cov": "LOG=no jest --coverage --runInBand --forceExit --detectOpenHandles",
    "watch": "nodemon . | pino-pretty",
    "start": "node . | pino-elasticsearch -i backend_node --host http://elastic.asacreative.com",
    "build": "tsc --build tsconfig.json",
    "build:watch": "tsc --build -w tsconfig.json",
    "test": "tsc --build tsconfig.json; jest --coverage --runInBand --forceExit --detectOpenHandles",
    "lint": "tslint -c tslint.json ./**/**/*.ts",
    "precommit": "npm run build && npm run lint && npm run test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "dotenv": "^8.2.0",
    "fastest-validator": "^1.0.0",
    "fs-extra": "^8.1.0",
    "knex": "^0.20.4",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-jwt": "^3.6.0",
    "koa-router": "^7.4.0",
    "luxon": "^1.21.3",
    "mysql": "^2.17.1",
    "objection": "^2.0.7",
    "on-finished": "^2.3.0",
    "pg": "^7.15.1",
    "pino": "^5.15.0",
    "pino-elasticsearch": "^4.2.0",
    "pino-pretty": "^3.5.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/koa-router": "^7.0.42",
    "@types/luxon": "^1.21.0",
    "@types/on-finished": "^2.3.1",
    "@types/pino": "^5.15.0",
    "@types/uuid": "^3.4.6",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "typescript": "^3.7.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/services/**/*",
      "!src/modules/index.js"
    ],
    "testEnvironment": "node"
  },
  "engines": {
    "node": "11.11.0"
  }
}
