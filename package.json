{
  "name": "backend-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test-cov": "LOG=no jest --coverage --runInBand --forceExit --detectOpenHandles",
    "watch": "nodemon . | pino-pretty",
    "start": "node . | pino-elasticsearch -i backend_node --host http://elastic.asacreative.com",
    "build": "tsc --build tsconfig.json",
    "build:watch": "tsc --build -w tsconfig.json",
    "test": "tsc --build tsconfig.json; jest --coverage --runInBand --forceExit --detectOpenHandles",
    "lint": "tslint -c tslint.json ./**/**/*.ts",
    "precommit": "npm run build && npm run lint && npm run test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "dotenv": "^6.2.0",
    "fastest-validator": "^0.6.16",
    "fs-extra": "^7.0.1",
    "knex": "^0.16.3",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-jwt": "^3.5.1",
    "koa-router": "^7.4.0",
    "luxon": "^1.11.4",
    "mysql": "^2.16.0",
    "objection": "^1.6.3",
    "on-finished": "^2.3.0",
    "pg": "^7.8.2",
    "pino": "^5.11.1",
    "pino-elasticsearch": "^3.1.0",
    "pino-pretty": "^2.5.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/koa-router": "^7.0.40",
    "@types/luxon": "^1.11.1",
    "@types/on-finished": "^2.3.1",
    "@types/pino": "^5.8.5",
    "@types/uuid": "^3.4.4",
    "jest": "^24.3.1",
    "nodemon": "^1.18.10",
    "supertest": "^4.0.0",
    "typescript": "^3.3.4000"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/services/**/*",
      "!src/modules/index.js"
    ],
    "testEnvironment": "node"
  },
  "engines": {
    "node": "11.11.0"
  }
}
